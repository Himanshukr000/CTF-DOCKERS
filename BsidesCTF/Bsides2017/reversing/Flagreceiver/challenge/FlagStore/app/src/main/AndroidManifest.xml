            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Send_to_Activity" />
        <activity android:name=".CTFReceiver"></activity>
    </application>

</manifest>